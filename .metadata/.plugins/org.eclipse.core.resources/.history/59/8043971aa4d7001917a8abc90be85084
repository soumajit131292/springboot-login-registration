package com.bridgelabz.dao;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import com.bridgelabz.model.UserDetailsForRegistration;

@Repository
public interface UserDao extends JpaRepository<UserDetailsForRegistration,Integer>{

	@Query(value="select * from user_registration_details")
	public List<UserDetailsForRegistration> retriveUserDetails();
	
	//public <S extends UserDetailsForRegistration> boolean exists(Example<S> example);
}
