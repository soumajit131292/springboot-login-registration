package com.bridgelabz.util;

import java.util.Date;

import com.auth0.jwt.JWT;
import com.bridgelabz.model.UserDetailsForRegistration;

public class TokenGeneration {

	private final long EXPIRATION_TIME=3000;
	
	public String generString(UserDetailsForRegistration email) {
	
	return	JWT.create().withClaim("emailId",email).withIssuedAt(new Date(System.currentTimeMillis())).withExpiresAt(new Date(System.currentTimeMillis()+EXPIRATION_TIME));
	
	}
}
}
