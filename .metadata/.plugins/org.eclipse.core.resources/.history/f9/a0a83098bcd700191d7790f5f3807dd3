package com.bridgelabz.dao;

import java.util.List;

import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.CrudRepository;
import org.springframework.stereotype.Repository;

import com.bridgelabz.model.UserDetailsForRegistration;

@Repository
public interface UserDao extends CrudRepository<UserDetailsForRegistration,Integer>{

	@Query(value="from user_registration_details")
	public List<UserDetailsForRegistration> retriveUserDetails();
	
}
